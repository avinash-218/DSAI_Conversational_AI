@prefix exampleURI: <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix relation: <http://purl.org/vocab/relationship/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

exampleURI:LISA a foaf:Person ;
    exampleURI:Age "48"^^xsd:int ;
    exampleURI:Character "STRICT" ;
    exampleURI:Completed "PHD" ;
    exampleURI:Guided exampleURI:JOHN ;
    exampleURI:Interest "DS" ;
    exampleURI:Occupation "TEACHER" ;
    exampleURI:Position "PRINCIPAL" ;
    exampleURI:Teaches exampleURI:DS,
        exampleURI:JOHN ;
    exampleURI:WorksAt "MSEC" .

exampleURI:MIKE a foaf:Person ;
    exampleURI:Age "18"^^xsd:int ;
    exampleURI:Character "KIND" ;
    exampleURI:Completed "ME" ;
    exampleURI:DoneProject "BOOSTING_TECHNIQUES" ;
    exampleURI:Interest "CODING" ;
    exampleURI:Occupation "STUDENT" ;
    exampleURI:Position "INTERN" ;
    exampleURI:StudiesAt "TCE" ;
    exampleURI:WorksAt "DEEPSPHERE" ;
    relation:friendOf exampleURI:JOHN .

exampleURI:JOHN a foaf:Person ;
    exampleURI:Age "22"^^xsd:int ;
    exampleURI:Character "FRIENDLY" ;
    exampleURI:Completed "BE" ;
    exampleURI:DoneProject "CONVERSATIONAL_AI" ;
    exampleURI:Interest "BLOGGING" ;
    exampleURI:Occupation "STUDENT" ;
    exampleURI:Position "INTERN" ;
    exampleURI:StudiesAt "MSEC" ;
    exampleURI:WorksAt "DEEPSPHERE" ;
    relation:friendOf exampleURI:MIKE .

